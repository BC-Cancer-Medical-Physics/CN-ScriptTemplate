<Window x:Class="VMS.TPS.ScriptWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestScript"
        mc:Ignorable="d"
        Title="My GUI" Height="139.44" Width="258.574" ResizeMode="NoResize">
    <Grid HorizontalAlignment="Left" Width="247" Margin="0,0,0,1">
        <Button x:Name="Button_Done" HorizontalAlignment="Left" Height="38" Margin="202,58,0,0" VerticalAlignment="Top" Width="35" Click="Close_GUI" RenderTransformOrigin="2.486,0.429" BorderBrush="{x:Null}" Background="{x:Null}">
            <Image Source="Resources/check.png" Stretch="Fill"/>
        </Button>
        <Label Content="Selected Plan:" HorizontalAlignment="Left" Height="29" Margin="8,9,0,0" VerticalAlignment="Top" Width="100" FontSize="14"/>
        <Label x:Name="label_PlanID" Content="Label" HorizontalAlignment="Left" Height="34" Margin="116,9,0,0" VerticalAlignment="Top" Width="126" FontWeight="Bold" FontSize="14">
            <Label.Foreground>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.HotTrackColorKey}}"/>
            </Label.Foreground>
        </Label>

    </Grid>
</Window>
